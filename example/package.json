{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "babel-node ./test/utils/run.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build": "npm run clean && npm run build:key && npm run build:release",
    "build:key": "keytool -genkey -v -keystore android/app/release.keystore -storepass android -alias androidreleasekey -keypass android -dname 'CN=Android Debug,O=Android,C=US'",
    "build:release": "cd android && ./gradlew assembleRelease --console=plain -S",
    "appium": "appium",
    "clean": "rm -rf android/app/release.keystore",
    "postinstall": "npm i appium --no-shrinkwrap"
  },
  "dependencies": {
    "react": "15.3.2",
    "react-native": "0.34.1",
    "tipsi-dropdown": "../"
  },
  "devDependencies": {
    "adbkit": "^2.6.0",
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "glob": "^7.1.0",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.2",
    "tape-async": "^2.1.1",
    "webdriverio": "^4.2.16"
  }
}
